# Definition for singly-linked list.
class ListNode:
    def __init__(self, val=0, next=None):
        self.val = val
        self.next = next
        
class Solution:
    def checkPalindrome(self, l1: ListNode):
      word=""
      while l1 is not None:
        word=word+str(l1.val)
        l1=l1.next
      newword=word[::-1]
      if word==newword:
        return True
      else:
        return False
        
        
    
    


    
    
    
    
    
